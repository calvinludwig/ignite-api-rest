{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "xo",
  "overrides": [
    {
      "extends": [
        "xo-typescript"
      ],
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {
        "@typescript-eslint/naming-convention": [
          "off"
        ]
      }
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {}
}
